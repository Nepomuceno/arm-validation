{
  "name": "arm-validation",
  "version": "0.1.0",
  "description": "Azure arm validation. basic command line tool to validate arm templates agains the chema files they belong",
  "scripts": {
    "compile": "tsc --experimentalDecorators --target es5 --module commonjs --noImplicitAny",
    "test": "mocha --opts mocha.opts",
    "prebuild": "npm i",
    "build": "rimraf bin/ & npm run compile",
    "precomplete": "npm run build",
    "complete": "npm run test"
  },
  "bin": {
    "azcosts": "bin/arm-validator.js"
  },
  "keywords": [
    "cli",
    "arm",
    "azure"
  ],
  "author": {
    "name": "Gabriel Nepomuceno",
    "email": "gabriel.bico@gmail.com"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nepomuceno/arm-validation.git"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/chai": "^4.1.3",
    "@types/inquirer": "0.0.41",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.4",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.0.0",
    "mocha": "^5.1.1",
    "rimraf": "^2.6.2",
    "sinon": "^5.0.7",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "ajv": "^6.4.0",
    "axios": "^0.18.0",
    "commander": "^2.15.1",
    "github-api": "^3.0.0",
    "inquirer": "^5.2.0",
    "npm": "^6.0.0",
    "simple-git": "^1.92.0",
    "ts-git": "^0.1.0"
  }
}
