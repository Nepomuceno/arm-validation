{
  "name": "arm-validation",
  "version": "0.1.0",
  "description": "Azure arm validation. basic command line tool to validate arm templates agains the chema files they belong",
  "scripts": {
    "compile": "tsc --experimentalDecorators --target es5 --module commonjs --noImplicitAny",
    "test": "mocha --opts mocha.opts",
    "prebuild": "npm i",
    "build": "rimraf bin/ & npm run compile",
    "precomplete": "npm run build",
    "complete":"npm run test"
  },
  "bin": {
    "azcosts": "bin/arm-validator.js"
  },
  "keywords": [
    "cli",
    "arm",
    "azure"
  ],
  "author": {
    "name": "Gabriel Nepomuceno",
    "email": "gabriel.bico@gmail.com"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nepomuceno/arm-validation.git"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/chai": "^3.4.35",
    "@types/commander": "^2.3.31",
    "@types/inquirer": "0.0.32",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "axios": "^0.16.1",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-plugin-mocha": "^4.8.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "ajv": "^4.11.6",
    "axios": "^0.16.1",
    "commander": "^2.9.0",
    "inquirer": "^3.0.1"
  }
}
