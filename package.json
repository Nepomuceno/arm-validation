{
  "name": "arm-validation",
  "version": "0.1.0",
  "description": "Azure arm validation. basic command line tool to validate arm templates agains the chema files they belong",
  "scripts": {
    "compile": "tsc --experimentalDecorators --target es5 --module commonjs --noImplicitAny",
    "test": "mocha --opts mocha.opts",
    "prebuild": "npm i",
    "build": "rimraf bin/ & npm run compile",
    "precomplete": "npm run build",
    "complete": "npm run test"
  },
  "bin": {
    "azcosts": "bin/arm-validator.js"
  },
  "keywords": [
    "cli",
    "arm",
    "azure"
  ],
  "author": {
    "name": "Gabriel Nepomuceno",
    "email": "gabriel.bico@gmail.com"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nepomuceno/arm-validation.git"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/chai": "^4.0.3",
    "@types/inquirer": "0.0.37",
    "@types/mocha": "^2.2.41",
    "@types/node": "^9.4.7",
    "chai": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^5.0.4",
    "rimraf": "^2.6.1",
    "sinon": "^4.4.6",
    "ts-node": "^5.0.1",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "ajv": "^6.2.1",
    "axios": "^0.18.0",
    "commander": "^2.11.0",
    "inquirer": "^5.1.0"
  }
}
